@using pojisteni_FULL.Utils;
@using pojisteni_FULL.Models.ViewModels;
@using pojisteni_FULL.Models.ViewModels.Items;
@model InsuredPersonListViewModel
@{
	ViewData["Title"] = "aplikace Pojištění";
}

<div class="col-12">
	
		<div class="col-12 mb-3 p-4 bg-light rounded border">
			<h1 class="display-4">@ViewData["Title"]</h1>
			<div class="text-left">
				<p>« správa pojištěnců a pojištění »</></p>
			</div>
		</div>
	
</div>

<div class="col-12">
	<div class="row">
		<div class="col">
			<div class="p-4 bg-light rounded border">
				<h2 class="display-6">Pojištěnci</h2>
				<p>« Přehled a správa pojištěnců »</p>
				<p>
					<a class="btn btn-default btn-outline-primary" asp-area="" asp-controller="InsuredPersons" asp-action="Index">
						Pojištěnci &raquo;
					</a>
				</p>
			</div>
		</div>
		<div class="col">
			<div class="p-4 bg-light rounded border">
				<h2 class="display-6">Pojištění</h2>
				<p>« Přehled a správa pojištění  »</p>
				<p>
					<a class="btn btn-default btn-outline-primary" asp-area="" asp-controller="Insurances" asp-action="Index">
						Pojištění &raquo;
					</a>
				</p>
			</div>
		</div>
	</div>
</div>





